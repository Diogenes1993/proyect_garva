	<subDataset name="New Dataset 1" uuid="1a2e3cc7-ff71-4995-9108-0fcee808f301">
		<queryString language="SQL">
			<![CDATA[SELECT
     BOLETA.`pk_id_boleta` AS BOLETA_pk_id_boleta,
     BOLETA.`fecha_boleta` AS BOLETA_fecha_boleta,
     BOLETA.`igv` AS BOLETA_igv,
     BOLETA.`fk_id_cliente` AS BOLETA_fk_id_cliente,
     BOLETA.`fk_id_empleado` AS BOLETA_fk_id_empleado,
     BOLETA.`fk_id_pedido` AS BOLETA_fk_id_pedido,
     BOLETA.`pago_consumo` AS BOLETA_pago_consumo
FROM
     `BOLETA` BOLETA]]>
		</queryString>
		<field name="BOLETA_fk_id_cliente" class="java.lang.String"/>
		<field name="BOLETA_fecha_boleta" class="java.time.LocalDateTime"/>
		<field name="BOLETA_pago_consumo" class="java.lang.Double"/>
		<group name="BOLETA_fk_id_cliente">
			<groupExpression><![CDATA[$F{BOLETA_fk_id_cliente}]]></groupExpression>
		</group>
		<group name="BOLETA_pago_consumo">
			<groupExpression><![CDATA[$F{BOLETA_pago_consumo}]]></groupExpression>
		</group>
		<group name="BOLETA_fecha_boleta">
			<groupExpression><![CDATA[$F{BOLETA_fecha_boleta}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="No_Boleta" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Cliente" class="java.lang.String"/>
