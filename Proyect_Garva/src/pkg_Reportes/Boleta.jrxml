	<parameter name="Total" class="java.lang.String"/>
	<parameter name="Monto" class="java.lang.String"/>
	<parameter name="Cajero" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT D.cantidad,M.nombre_menu,CONCAT('S/ ',M.precio_menu),CONCAT('S/ ',(M.precio_menu*D.cantidad)) AS importe FROM BOLETA AS B   
INNER JOIN CLIENTE AS C ON C.pk_id_cliente = B.fk_id_cliente 
INNER JOIN EMPLEADO AS E ON E.pk_id_empleado = B.fk_id_empleado  
INNER JOIN DETALLE_PEDIDO AS D ON B.fk_id_pedido = D.fk_id_pedido 
INNER JOIN MENU AS M ON M.pk_id_menu = D.fk_id_menu 
WHERE B.pk_id_boleta =$P{No_Boleta}
ORDER BY 1 ASC;]]>
	</queryString>
	<field name="cantidad" class="java.lang.Long">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="nombre_menu" class="java.lang.String">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="CONCAT(&apos;S/ &apos;,M.precio_menu)" class="java.lang.String"/>
	<field name="importe" class="java.lang.String"/>
